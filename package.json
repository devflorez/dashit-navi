{
  "name": "dashit-navi",
  "version": "0.0.0",
  "license": "MIT",
  "bin": "dist/cli.js",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "prettier --check . && xo && ava"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "ink": "^5.2.0",
    "meow": "^13.2.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^7.0.0",
    "@types/react": "^19.1.2",
    "@vdemedes/prettier-config": "^2.0.1",
    "ava": "^6.2.0",
    "chalk": "^5.2.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ink-testing-library": "^3.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "xo": "^0.53.1"
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "xo": {
    "extends": "xo-react",
    "prettier": true,
    "rules": {
      "react/prop-types": "off"
    }
  },
  "prettier": "@vdemedes/prettier-config",
  "packageManager": "npm@11.1.0"
}